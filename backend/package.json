{
  "name": "prescription-server",
  "version": "1.0.0",
  "description": "TypeScript Express server with MCP integration for prescription generation",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "postinstall": "npm run build",
    "start": "npm run build && node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "mcp": "node dist/mcp-server.js",
    "mcp:dev": "tsx src/mcp-server.ts"
  },
  "dependencies": {
    "@google/genai": "^0.8.0",
    "@huggingface/inference": "^4.0.6",
    "@modelcontextprotocol/sdk": "^1.13.1",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "mongoose": "^8.16.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "firebase-admin": "^12.0.0",
    "jsonwebtoken": "^9.0.2",
    "pdfkit": "^0.14.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.23",
    "@types/node": "^20.19.1",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/pdfkit": "^0.13.4",
    "@types/multer": "^1.4.11",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.10",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^7.0.0"
  },
  "bin": {
    "prescription-mcp": "./dist/mcp-server.js"
  }
}
